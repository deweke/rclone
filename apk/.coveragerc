[run]
branch = True
omit =
    # Omit virtualenv directory.
    venv/*
    # Omit scripts directory.
    scripts/*
    # Omit test directory.
    test/*
    # Omit protobuf generated files.
    *_pb2.py
    # Omit web version.
    flask_app.py

[report]
exclude_lines =
    if __name__ == .__main__.:
